{% block body %}
    <div id="{{ rootId }}"
        class="progressBarRange progress"{% if (media.getDuration() > 0) %}
        data-media-time_start="0"
        data-media-time_end="{{ media.getDuration() }}"
        data-progress-time_start="0"
        data-progress-time_end="{{ min (60, media.getDuration() ) }}"
        onclick="ttgMoveInsideRange('{{ rootId }}', '{{ playerHtmlId }}')"
    {% endif %}
    ><div id="{{ rootId }}Before"
          class="progressBarRangeBefore progress-bar progress-bar-striped active"
        ></div><span id="{{ rootId }}After" class="progressBarRangeAfter"></span></div>

{% endblock %}
